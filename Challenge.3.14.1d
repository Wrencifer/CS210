#include <iostream>
#include <string>
using namespace std;

int main() {
   string userGreeting;
   string userName;

   getline(cin, userGreeting);
   getline(cin, userName);

   userGreeting.append(" ");
   userGreeting.append(userName);
   userGreeting.append("!");

   cout << userGreeting << endl;

   return 0;
}
