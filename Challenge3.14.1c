#include <iostream>
#include <string>
using namespace std;

int main() {
   string userInput1;
	string userInput2;
	int maxSize;
   
   getline(cin, userInput1);
	getline(cin, userInput2);
   
   if (userInput1.size() > userInput2.size()) {
      maxSize = userInput1.size();
   }
   else {
      maxSize = userInput2.size();
   }
	cout << maxSize << endl;

   return 0;
}
